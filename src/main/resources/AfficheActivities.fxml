<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controllers.AfficheActiviteController">

    <TableView fx:id="tablev" layoutX="50.0" layoutY="50.0" prefHeight="300.0" prefWidth="500.0">
        <columns>
            <TableColumn fx:id="colid" text="ID" prefWidth="150.0"/>
            <TableColumn fx:id="colname" text="Name" prefWidth="200.0"/>
            <TableColumn fx:id="colActions" text="Actions" prefWidth="150.0"/>
            <TableColumn fx:id="colDelete" text=""/>


        </columns>
    </TableView>

    <Button layoutX="250.0" layoutY="360.0" text="Back" onAction="#Back"/>

</AnchorPane>
