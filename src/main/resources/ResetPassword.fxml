<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ResetPasswordController">
    <children>
        <Text layoutX="200.0" layoutY="50.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Reset Password" textAlignment="CENTER">
            <font>
                <Font name="Arial Bold" size="24.0" />
            </font>
        </Text>
        <TextField fx:id="email" layoutX="150.0" layoutY="100.0" promptText="Email" />
        <PasswordField fx:id="newPassword" layoutX="150.0" layoutY="150.0" promptText="New Password" />
        <PasswordField fx:id="confirmPassword" layoutX="150.0" layoutY="200.0" promptText="Confirm Password" />
        <Button fx:id="resetButton" layoutX="250.0" layoutY="250.0" mnemonicParsing="false" onAction="#resetPassword" text="Reset Password" />
    </children>
</AnchorPane>